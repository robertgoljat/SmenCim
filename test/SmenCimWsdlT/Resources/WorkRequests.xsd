    </xs:restriction>
  </xs:simpleType>
  <xs:element name="WorkTimeSchedule" type="el:WorkTimeSchedule" />
  <xs:complexType name="WorkTimeSchedule">
    <xs:annotation>
      <xs:documentation>Time schedule specific to work.</xs:documentation>
    </xs:annotation>
